{
  "name": "netlify-plugin-maxmind-geoip-csv",
  "version": "1.0.0",
  "description": "A Netlify plugin that processes and compresses GeoIP databases for enhanced precision in Canadian geolocations, outputting optimized JSON files for Netlify geolocation functions.",
  "license": "MIT",
  "author": "Thinkflo",
  "type": "module",
  "exports": "./src/index.js",
  "main": "./src/index.js",
  "files": [
    "src/**/*.js",
    "src/**/*.json",
    "src/**/*.sh",
    "src/**/*.html",
    "src/**/*.ejs",
    "manifest.yml"
  ],  
  "keywords": [
    "netlify",
    "netlify-plugin",
    "geo-ip",
    "geolocation",
    "maxmind",
    "csv-processing",
    "canadian-data"
  ],
  "scripts": {
    "build": "netlify-build",
    "test": "npm run lint && npm run ava",
    "lint": "npm run eslint && npm run prettier",
    "eslint": "eslint --ignore-path .gitignore --fix --cache --format=codeframe --max-warnings=0 \"{src,init}/**/*.{cjs,mjs,js}\"",
    "prettier": "prettier --ignore-path .gitignore --write --loglevel warn \"{.github,src,init}/**/*.{cjs,mjs,js,md,yml,json}\" \"*.{cjs,mjs,js,md,yml,json}\"",
    "ava": "cross-env FORCE_COLOR=1 ava --verbose",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thinkflo/netlify-plugin-graphql-algolia.git"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "adm-zip": "^0.5.10"
  },
  "devDependencies": {
    "lodash.chunk": "^4.2.0",
    "@netlify/build": "^26.5.3",
    "ava": "^4.0.0",
    "chalk": "^5.0.0",
    "cpy": "^9.0.0",
    "cross-env": "^7.0.2",
    "del": "^6.0.0",
    "email-validator": "^2.0.4",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "execa": "^6.0.0",
    "fast-glob": "^3.2.4",
    "filter-obj": "^3.0.0",
    "fuzzy": "^0.1.3",
    "git-remote-origin-url": "^4.0.0",
    "inquirer": "^8.0.0",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "netlify-cli": "^9.16.2",
    "omit.js": "^2.0.2",
    "prettier": "^2.1.2",
    "release-it": "^15.0.0",
    "semver": "^7.3.2",
    "spdx-license-list": "^6.3.0",
    "strip-ansi": "^7.0.0",
    "username": "^6.0.0",
    "yargs": "^17.0.0"
  },
  "ava": {
    "verbose": true,
    "timeout": "100s",
    "environmentVariables": {
      "FORCE_COLOR": "1"
    }
  }
}